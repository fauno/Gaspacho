config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option macaddr '6c:e8:73:d2:a9:9c'
	option hwmode '11ng'
	option htmode 'HT20'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option disabled '0'

# conexion para clientes
config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option ssid 'Gaspacho'

# conexion a un Punto de Acceso
config wifi-iface
	option device 'radio0'
	option network 'wan'
	option mode 'sta'
	option ssid 'EDUARDO'
	option encryption 'psk'
	option key '080158bb781f0865c4b7e9886e2a48a8607a9ab020476e3dd906aa110daed82a'

